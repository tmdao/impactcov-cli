{
  "name": "impactcov-cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "tia-cli": "dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --target node18 --dts --sourcemap --clean",
    "dev": "tsx src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "test": "node --version"
  },
  "dependencies": {
    "@iarna/toml": "^3.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "execa": "^9.3.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "ora": "^8.0.0",
    "picocolors": "^1.0.0",
    "undici": "^6.19.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.30",
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsup": "^8.0.2",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.46.2"
  },
  "engines": {
    "node": ">=18.18"
  },
  "packageManager": "pnpm@9.0.0",
  "license": "MIT"
}
